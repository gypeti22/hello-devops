{
  "name": "PHP Hello World Codespaces",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "workspaceFolder": "/workspace",
  "extensions": [
    "felixfbecker.php-debug",
    "bmewburn.vscode-intelephense-client"
  ],
  "forwardPorts": [80],
  "settings": {
    "php.validate.executablePath": "/usr/local/bin/php"
  }
}